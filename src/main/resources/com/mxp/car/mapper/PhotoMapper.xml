<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mxp.car.mapper.PhotoMapper">

    <insert id="insertList" parameterType="Photo">
        INSERT INTO photo(photo_id,car_id,name,url,order_id)
        VALUES
        <foreach collection="list" item="listP" separator="," >
            (#{listP.photoId},#{listP.carId},#{listP.name},#{listP.url},#{listP.orderId})
        </foreach>
    </insert>
</mapper>
